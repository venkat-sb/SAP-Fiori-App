{
	"_version": "1.7.0",
	"sap.app": {
		"id": "myApp",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "myApp",
			"version": "1.40.12"
		},
		"tags": {
			"keywords": ["{{bigData}}", "{{health}}"]
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_belize", "sap_bluecrystal"],
		"icons":{
			"icon":"sap-icon://accept"
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.m": {"minUI5Version": "1.30.0"},
				"sap.ui.layout": {"minUI5Version": "1.30.0"}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"rootView":{
			"viewName":"ibm.fin.ar.view.App",
			"id":"idRootView",
			"type":"XML"	
		},
		"routing":{
			"config":{
				 "routerClass":"sap.m.routing.Router",
				 "viewType":"XML",
				 "viewPath":"ibm.fin.ar.view",
				 "controlId":"myApp"
			},
			"routes":[
				{
					"pattern":"",
					"name":"venkat",
					"target":["donald"]
					},
					{
					"pattern":"zumba/{myVar}",
					"name":"smith",
					"target":["donald","trumph"]
					}
					],
					
					"targets":{
				 		"donald":{
							"viewName":"View1",
							"controlAggregation":"masterPages",
							"viewId":"idView1"
					},
					"trumph":{
							"viewName":"View2",
							"controlAggregation":"detailPages",
							"viewId":"idView2"
					}
					}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {},
				"uri": "i18n/i18n.properties",
				"preload": true,
				"async": true
			},
			"": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "model/mockData/bigData.json",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css",
				"id":"mycss"
			}]
		}
	}
}